{
  "name": "earning-api",
  "version": "1.0.0",
  "description": "REST API for LSEG task",
  "main": "src/index.ts",
  "scripts": {
    "build:check": "tsc --project ./tsconfig.json --noEmit",
    "start": "ts-node src/index.ts",
    "start:dev": "nodemon --exec ts-node src/index.ts",
    "start:w": "nodemon --watch --exec ts-node src/index.ts",
    "test": "jest",
    "lint:fix": "eslint --fix ./src",
    "generate-interfaces-for-joi": "ts-node ./generateShemaInterfaces.ts"
  },
  "jest": {
    "transform": {
      "\\.[jt]sx?$": "babel-jest"
    }
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "**/*.{js,ts}": [
      "bash -c 'npm run build:check'",
      "prettier --write",
      "eslint"
    ]
  },
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@babel/core": "^7.11.6",
    "@babel/node": "^7.10.5",
    "@babel/preset-env": "^7.11.5",
    "@babel/preset-typescript": "^7.12.17",
    "@types/bcryptjs": "^2.4.2",
    "@types/bluebird": "^3.5.33",
    "@types/cors": "^2.8.10",
    "@types/express": "^4.17.11",
    "@types/intl": "^1.2.0",
    "@types/jest": "^26.0.20",
    "@types/node": "^14.14.28",
    "@types/swagger-ui-express": "^4.1.2",
    "@types/validator": "^13.1.3",
    "@typescript-eslint/eslint-plugin": "^4.15.0",
    "@typescript-eslint/parser": "^4.15.0",
    "babel-jest": "^27.0.6",
    "dotenv": "^8.2.0",
    "eslint": "^7.20.0",
    "husky": "^4.3.8",
    "jest": "^27.3.1",
    "joi-to-typescript": "^1.11.0",
    "lint-staged": "^10.5.4",
    "nodemon": "^2.0.4",
    "prettier": "^2.2.1",
    "rimraf": "^3.0.2",
    "ts-node": "^10.9.1",
    "typescript": "^5.1.6"
  },
  "dependencies": {
    "@types/dateformat": "^5.0.0",
    "@types/express-brute": "^1.0.1",
    "@types/express-session": "^1.17.4",
    "axios": "^0.21.4",
    "axios-mock-adapter": "^1.21.5",
    "bcrypt": "^5.0.0",
    "bcryptjs": "^2.4.3",
    "cors": "^2.8.5",
    "csv-parse": "^5.5.0",
    "date-fns": "^2.17.0",
    "dateformat": "^5.0.3",
    "express": "^4.17.1",
    "express-brute": "^1.0.1",
    "express-ipfilter": "^1.1.2",
    "http-status-codes": "^2.1.4",
    "intl": "^1.2.5",
    "joi": "^17.4.0",
    "joi-to-swagger": "^5.2.0",
    "jsforce": "^1.10.0",
    "jszip": "^3.7.1",
    "log4js": "^6.3.0",
    "mime-types": "^2.1.31",
    "moment": "^2.29.1",
    "node-cron": "^3.0.2",
    "object-path": "^0.11.5",
    "swagger-ui-express": "^4.1.6"
  }
}
